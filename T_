
# DEVATALA CHARAN TEJA   [MCSAIR   ROLLNO:37   ADCKKD]
n = int(input('ENTER SIZE OF LIST::'))
l = []
a = []
for i in range(0, n):
    i = int(input('ENTER ELEMENT::'))
    l.append(i)

'''l = [1, 2, 3, 4, 565, 34, 76]
'''
for j in range(0, n):
    if l[j] % 2 == 0:
        p = l[j]
        f = False
        if l[j] > 1:
            for k in range(2, p):
                if p % k == 0:
                    f == True
                    break
if f:
    a.append(p)

print(a)
